fault();\n    fetchArtist();\n  }\n\n  // helper function for number of followers\n  function shortenNum(num) {\n    let str = num + \"\";\n    if (str.length >= 7) {\n      let integer = Math.floor(num / 1000000) + \".\";\n      let decimal = (num % 1000000) + \"\";\n      return integer + decimal.substring(0, 1) + \"M\"\n    } else if (str.length >= 6) {\n      let integer = Math.floor(num / 1000);\n      return integer + \"K\"\n    } else if (str.length >= 4) {\n      let integer = Math.floor(num / 1000) + \".\";\n      let decimal = (num % 1000) + \"\";\n      return integer + decimal.substring(0, 1) + \"K\"\n    } else {\n      return str;\n    }\n  }\n\n  // helper function for music genre tags\n  function displayTags(tags) {\n    let result = [];\n    for (let tag of tags) {\n      result.push(\n        <div key={tag} className=\"artist-tag\">\n          <p>{tag}</p>\n        </div>\n      )\n    } return result;\n  }\n\n  // handle case when song is an empty object\n  if (typeof (artist) === \"undefined\" || Object.keys(artist).length === 0) {\n    return (\n      <div className=\"header\" >\n        <div className=\"back\" />\n        <div className=\"top\">\n          <div className=\"navigation\">\n            <h1>Music</h1>\n            <form className=\"search-bar\"\n              onSubmit={handleSubmit}>\n              <input\n                type=\"text\"\n                placeholder=\"Search\"\n                value={searchTerm}\n                onChange={(event) => setSearchTerm(event.target.value)}\n              />\n              <button className=\"search-bar-button\" type=\"submit\">\n                <BiSearch className=\"search-bar-icon\" />\n              </button>\n            </form>\n          </div>\n        </div>\n      </div>\n    )\n  } else {\n\n    var headerStyle = {\n      backgroundImage: \"url(\" + artist.images[0].url + \")\"\n    };\n\n    return (\n      <div className=\"header\" >\n        <div className=\"back\" style={headerStyle} />\n        <div className=\"top\">\n          <div className=\"navigation\">\n            <h1><a href=\"/\">Music</a></h1>\n            <form className=\"search-bar\"\n              onSubmit={handleSubmit}>\n              <input\n                type=\"text\"\n                placeholder=\"Search\"\n                value={searchTerm}\n                onChange={(event) => setSearchTerm(event.target.value)}\n              />\n              <button className=\"search-bar-button\" type=\"submit\">\n                <BiSearch className=\"search-bar-icon\" />\n              </button>\n            </form>\n          </div>\n          <div className=\"artist\">\n            <img className=\"artist-img\" src={artist.images[1].url} alt={artist.images[1].url} />\n            <div className=\"artist-info\">\n              <h2>{artist.name}</h2>\n              <div className=\"artist-detail\">\n                <p><span className=\"larger\"><FiMusic className=\"artist-music-icon\" />{shortenNum(artist.followers.total)}</span> followers</p>\n                <div className=\"artist-tags\">{displayTags(artist.genres)}</div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div >\n    )\n  }\n}\n","/Users/yubin/Desktop/yubinary/music-player-2.0/music/src/pages/Search.js",[],{"ruleId":"48","replacedBy":"49"},{"ruleId":"50","replacedBy":"51"},{"ruleId":"52","severity":1,"message":"53","line":29,"column":6,"nodeType":"54","endLine":29,"endColumn":16,"suggestions":"55"},{"ruleId":"56","severity":1,"message":"57","line":1,"column":17,"nodeType":"58","messageId":"59","endLine":1,"endColumn":26},"no-native-reassign",["60"],"no-negated-in-lhs",["61"],"react-hooks/exhaustive-deps","React Hook useEffect has missing dependencies: 'audio', 'setPlaylist', 'setSongToPlay', and 'song'. Either include them or remove the dependency array. If 'setPlaylist' changes too often, find the parent component that defines it and wrap that definition in useCallback.","