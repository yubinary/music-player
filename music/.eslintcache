n        setPlaylist(playlist.slice(1));\n      }\n    }\n  }, [playlist])\n\n  // change state: pause music when playing, play music when paused\n  function handleClick() {\n    if (!isPlay) {\n      audio.play();\n      setIsPlay(true);\n    } else {\n      audio.pause();\n      setIsPlay(false);\n    }\n  }\n\n  // show pause button when playing, play button when paused\n  function displayButton() {\n    if (isPlay) {\n      return (\n        <button className=\"play\" onClick={handleClick}><BsFillPauseFill /></button>\n      )\n    } else {\n      return (\n        <button className=\"play\" onClick={handleClick}><BsFillPlayFill /></button>\n      )\n    }\n  }\n\n  // helper function for conditional animation\n  function isAnimation(title) {\n    if (title.length > 19) {\n      return (\n        <h1 className=\"animate-h1\">{song.name}</h1>\n      )\n    } else {\n      return (\n        <h1 className=\"noanimate-h1\">{song.name}</h1>\n      )\n    }\n  }\n\n  // handle case when song is an empty object\n  if (Object.keys(song).length === 0) {\n    return (\n      <div className=\"player\">\n      </div>\n    )\n  } else {\n    return (\n      <div className=\"player\">\n        <div className=\"player-song\">\n          <div className=\"player-img\">\n            <img className=\"player-album\" src={song.album.images[1].url} alt={song.album.images[1].url} />\n            <img className=\"player-disk\" src={disk} alt=\"disk\" />\n          </div>\n          <div className=\"player-song-info\">\n            {isAnimation(song.name)}\n            <p>{cropParagraph(joinArray(song.artists), 27)}</p>\n          </div>\n          <div>\n            <button className=\"skip\">\n              <BsFillSkipStartFill />\n            </button>\n            {displayButton()}\n            <button className=\"skip\">\n              <BsFillSkipEndFill />\n            </button>\n          </div>\n        </div>\n        <Playlist\n          playlist={playlist}\n          setPlaylist={setPlaylist}\n          cropParagraph={cropParagraph}\n        />\n      </div>\n    )\n  }\n}","/Users/yubin/Desktop/yubinary/music-player-2.0/music/src/components/Album.js",[],"/Users/yubin/Desktop/yubinary/music-player-2.0/music/src/components/Song.js",[],"/Users/yubin/Desktop/yubinary/music-player-2.0/music/src/components/Playlist.js",[],"/Users/yubin/Desktop/yubinary/music-player-2.0/music/src/components/Header.js",["47"],"/Users/yubin/Desktop/yubinary/music-player-2.0/music/src/pages/Search.js",[],{"ruleId":"48","replacedBy":"49"},{"ruleId":"50","replacedBy":"51"},{"ruleId":"52","severity":1,"message":"53","line":29,"column":6,"nodeType":"54","endLine":29,"endColumn":16,"suggestions":"55"},{"ruleId":"56","severity":1,"message":"57","line":1,"column":17,"nodeType":"58","messageId":"59","endLine":1,"endColumn":26},"no-native-reassign",["60"],"no-negated-in-lhs",["61"],"react-hooks/exhaustive-deps","React Hook useEffect has missing dependencies: 'audio', 'setPlaylist', 'setSongToPlay', and 'song'. Either include them or remove the dependency array. If 'setPlaylist' changes too often, find the parent component that defines it and wrap that definition in useCallback.","ArrayExpression",["62"],"no-unused-vars","'useEffect' is defined but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation",{"desc":"63","fix":"64"},"Update the dependencies array to be: [audio, playlist, setPlaylist, setSongToPlay, song]",{"range":"65","text":"66"},[858,868],"[audio, playlist, setPlaylist, setSongToPlay, song]"]